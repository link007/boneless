### Configuración de diagnóstico BONELESS ###

app {
  name: "ZF Analyzer"
  version: 1.3.2
  author: Tomas
  last_update: @2025-11-04
}

settings {
  logging: true
  theme: dark
  thresholds {
    temperature: °90
    voltage: 12.6v
  }
}

solenoids {
  - id: A
    resistance: 5.1Ω
    type: linear
  - id: B
    resistance: 5.3Ω
    type: on/off
}
